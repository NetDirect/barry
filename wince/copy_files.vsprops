<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="copy_files"
	>
	<!--
		This property sheet is used to copy the barry headers and library to a directory suitable for tools to use.
	-->
	<Tool
		Name="VCPostBuildEventTool"
		CommandLine="cd $(SolutionDir) &amp; copy_files.bat &quot;$(PlatformName)&quot; &quot;$(ConfigurationName)&quot;"
	/>
</VisualStudioPropertySheet>
