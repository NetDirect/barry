connect "/usr/sbin/chat -f /etc/chatscripts/barry-vodafone-au.chat"
pty "/usr/sbin/pppob"

debug

noauth
user ""
password ""

defaultroute
usepeerdns

noipdefault
updetach
nodeflate
nobsdcomp
noaccomp
nocrtscts
nopcomp
nomagic
nomultilink

ipcp-restart 7
ipcp-accept-local
ipcp-accept-remote
ipcp-max-configure 30
lcp-echo-interval 0
lcp-echo-failure 999

passive
mtu 1492

